
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id            String         @id @default(cuid())
  name          String?
  email         String?        @unique
  password      String?
  image         String?
  phone_number  String?
  status        String         @default("Active")
  role          String?        @default("employee")
  createdAt     DateTime       @default(now())
  updatedAt     DateTime       @updatedAt
}
